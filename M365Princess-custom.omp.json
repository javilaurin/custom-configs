{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#000000",
    "blush": "#DA627D",
    "green": "#338a50",
    "php": "#7A86B8",
    "plum": "#9A348E",
    "red": "#CD0505",
    "salmon": "#FCA17D",
    "sky": "#86BBD8",
    "tan": "#CC3802",
    "teal_blue": "#33658A",
    "teal": "#047E84",
    "white": "#FFFFFF",
    "yellow": "#ffdc51"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground": "p:black",
          "background": "p:tan",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}"
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "p:black",
          "background": "p:teal",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .Env.PNPPSSITE }} \uf2dd {{ .Env.PNPPSSITE }}{{ end }}"
        },
        {
          "type": "text",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:teal",
          "template": "{{ if .Env.PNPPSSITE }}\u00A0{{ end }}"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:plum",
          "foreground": "p:black",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:blush",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:salmon",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": false,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "powerline",
          // "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "template": " \u279c {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} | {{ end }}{{ if .Staging.Changed }} \uf046{{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:sky",
          "display_mode": "files",
          "fetch_package_manager": true,
          "foreground": "p:black",
          "missing_command_text": "nodejs not found",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "type": "php",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "foreground": "p:black",
          "background": "p:php",
          "template": " \ue73d {{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "p:teal_blue",
          "template": " \ue606 {{ .Full }} "
        },
        {
          "background": "p:green",
          "foreground": "p:white",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " \u2665 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        },
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "p:white",
          "background": "p:red"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}